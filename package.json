{
  "name": "isig",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.7.5",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.7.1",
    "modernizr-webpack-plugin": "^1.0.5",
    "node-bourbon": "^4.2.3",
    "node-neat": "^2.0.0-beta.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "react-input-mask": "^0.8.0",
    "react-scripts": "1.0.7",
    "smoothscroll-polyfill": "^0.3.5"
  },
  "scripts": {
    "build-css": "node-sass src/scss/style.scss src/style.css",
    "watch-css": "npm run build-css && node-sass -w src/scss/style.scss src/style.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
